{
    "version": 2,
    "commands": [
        {
            "id": "wwiseauthoringutility.001.connecttolocalhost",
            "displayName": "Connect/Disconnect to Localhost",
            "program": "py ${CurrentCommandDirectory}\\wwise-authoring-utility\\Python\\WAU_ConnectToLocalHost.pyw",
            "defaultShortcut": "Alt+H",
            "redirectOutputs": true,
            "startMode": "SingleSelectionSingleProcess",
            "mainMenu": {
                "basePath": "WwiseUtility"
            }
        },
        {
            "id": "wwiseutility.wwiseauthoringutility.002.createrandomcontainerfromselectedobject",
            "displayName": "Create RandomContainer with selected objects",
            "program": "py ${CurrentCommandDirectory}\\wwise-authoring-utility\\Python\\WAU_MakeContainerFromSelectedObject.pyw",
            "args": "random",
            "defaultShortcut": "Alt+R",
            "redirectOutputs": true,
            "startMode": "MultipleSelectionSingleProcessSpaceSeparated",
            "contextMenu": {
                "basePath": "WwiseUtility",
                "visibleFor": "Sound,BlendContainer,RandomSequenceContainer,SwitchContainer"
            }
        },
        {
            "id": "wwiseutility.wwiseauthoringutility.003.autoassignswitchobject",
            "displayName": "Auto assign to Switch/State",
            "program": "py ${CurrentCommandDirectory}\\wwise-authoring-utility\\Python\\WAU_AssignSwitchObject.pyw",
            "args": "",
            "defaultShortcut": "Alt+W",
            "redirectOutputs": true,
            "startMode": "MultipleSelectionSingleProcessSpaceSeparated",
            "contextMenu": {
                "basePath": "WwiseUtility",
                "visibleFor": "BlendContainer,MusicPlaylistContainer,MusicSwitchContainer,RandomSequenceContainer,SwitchContainer"
            }
        },
        {
            "id": "wwiseutility.wwiseauthoringutility.004.customassignswitchcontainer",
            "displayName": "Custom assign to Switch/State",
            "program": "${CurrentCommandDirectory}\\Python\\Main.py",
            "args": "switch_custom ${id}",
            "defaultShortcut": "Ctrl+Alt+W",
            "redirectOutputs": true,
            "startMode": "MultipleSelectionSingleProcessSpaceSeparated",
            "contextMenu": {
                "basePath": "WwiseUtility",
                "visibleFor": "SwitchContainer"
            }
        },
        {
            "id": "wwiseutility.z.Print",
            "displayName": "Print",
            "program": "py ${CurrentCommandDirectory}\\wwise-authoring-utility\\Python\\WAU_Test.py",
            "args": "${id} ${name} ${type} ${path} ${parent}",
            "redirectOutputs": true,
            "startMode": "MultipleSelectionSingleProcessSpaceSeparated",
            "contextMenu": {
                "basePath": "WwiseUtility",
                "visibleFor": "Sound,BlendContainer,MusicPlaylistContainer,MusicSwitchContainer,RandomSequenceContainer,SwitchContainer"
            }
        },
        {
            "id": "wwiseutility.z.help",
            "displayName": "Help",
            "program": "${CurrentCommandDirectory}\\Python\\Main.py",
            "args": "help",
            "redirectOutputs": false,
            "mainMenu": {
                "basePath": "WwiseUtility"
            }
        }
    ]
}